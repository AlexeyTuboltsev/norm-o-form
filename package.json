{
  "name": "norm-o-form",
  "repository": "git://github.com/alexeyTuboltsev/norm-o-form.git",
  "version": "0.0.1",
  "private": true,
  "main": "src/index.ts",
  "dependencies": {},
  "scripts": {
    "build:install:dev": "tsc && cd examples/array && rm -fr node_modules/norm-o-form &&  yarn install --check-files && cd ../..",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "react-app-rewired build",
    "test": "jest",
    "eject": "react-scripts eject",
    "format": "prettier --list-different 'src/kvinta/**/*.{ts,tsx,js,json,md,gql}'",
    "format:fix": "prettier --write 'src/kvinta/**/*.{ts,tsx,js,json,md,gql}'",
    "lint:precommit": "lint-staged"
  },
  "pre-commit": [
    "lint:precommit"
  ],
  "lint-staged": {
    "src/kvinta/**/*.{ts,tsx,js,json,md,gql}": "prettier --write"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "compression-webpack-plugin": "^2.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^29.4.3",
    "lint-staged": "^11.1.2",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.5",
    "source-map-explorer": "^2.5.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5",
    "yarn": "^1.22.10"
  }
}
